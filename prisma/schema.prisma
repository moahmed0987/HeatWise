generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider  = "postgresql"
  url       = env("POSTGRES_PRISMA_URL")
  directUrl = env("POSTGRES_URL_NON_POOLING")
}

model HeatDemand {
  id           String @id
  lsoa11cd     String
  beforeDemand Float
  afterDemand  Float
  lsoa         lsoa   @relation(fields: [lsoa11cd], references: [lsoa11cd], onDelete: NoAction, onUpdate: NoAction)
}

model lsoa {
  lsoa11cd    String       @id @unique(map: "unique_lsoa11cd")
  lsoa11nm    String?
  lad17cd     String?
  lad17nm     String?
  heatDemands HeatDemand[]
}
