generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider  = "postgresql"
  url       = env("POSTGRES_PRISMA_URL")
  directUrl = env("POSTGRES_URL_NON_POOLING")
}

model HeatDemand {
  lsoa11cd     String @id(map: "HeatDemand_pkey1")
  beforeDemand Float?
  afterDemand  Float?
  lsoa         lsoa   @relation(fields: [lsoa11cd], references: [lsoa11cd], onDelete: NoAction, onUpdate: NoAction, map: "lsoa11cd___fk")
}

model lsoa {
  lsoa11cd      String          @id @unique(map: "unique_lsoa11cd")
  lsoa11nm      String?
  lad17cd       String?
  lad17nm       String?
  heatDemands   HeatDemand?
  HeatDemandOld HeatDemandOld[]
}

model HeatDemandOld {
  id           String @id(map: "HeatDemand_pkey")
  lsoa11cd     String
  beforeDemand Float
  afterDemand  Float
  lsoa         lsoa   @relation(fields: [lsoa11cd], references: [lsoa11cd], onDelete: NoAction, onUpdate: NoAction, map: "HeatDemand_lsoa11cd_fkey")
}
